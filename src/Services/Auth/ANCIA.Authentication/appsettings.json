{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TokenRules": {
    "Audience": "http://localhost",
    "ExpirationInMinutes": 20,
    "RefreshTokenHeader": "X-Refresh-Token",
    "NewAuthTokenHeader": "X-New-Auth-Token",
    "NewRefreshTokenHeader": "X-New-Refresh-Token"
  }
}
